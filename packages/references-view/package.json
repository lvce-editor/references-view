{
  "name": "@lvce-editor/references-view",
  "version": "0.0.0-dev",
  "description": "References View",
  "keywords": [
    "debug",
    "Lvce Editor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvce-editor/references-view.git"
  },
  "license": "MIT",
  "author": "Lvce Editor",
  "type": "module",
  "main": "src/referencesViewWorkerMain.ts",
  "scripts": {
    "build:watch": "cd ../../ && npm run build:watch",
    "dev": "cd ../../ && npm run dev",
    "test": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "test:watch": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch"
  },
  "jest": {
    "injectGlobals": false,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    }
  },
  "devDependencies": {
    "@jest/globals": "^30.0.3",
    "@lvce-editor/assert": "^1.4.0",
    "@lvce-editor/i18n": "^1.1.0",
    "@lvce-editor/rpc": "^4.5.0",
    "@lvce-editor/rpc-registry": "^2.34.0",
    "@lvce-editor/verror": "^1.7.0",
    "@lvce-editor/viewlet-registry": "^1.2.0",
    "@lvce-editor/virtual-dom-worker": "^2.3.0",
    "jest": "^30.0.3",
    "ts-jest": "^29.4.0"
  }
}
